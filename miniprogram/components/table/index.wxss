.table-outer-box {
display: flex;
flex-direction: column;
padding: 50rpx 50rpx 0;
flex: 1;
}

.table {
  height: 80rpx;
  color: #000;
  line-height: 80rpx;
  border-top: 1rpx solid #5f677a;
  border-bottom: 1rpx solid #5f677a;
  text-align: center;
}

.table-flex {
display: flex;
}

.table-theme {
font-size: 28rpx;
}

.table-item {
font-size: 24rpx;
opacity: 0.7;
}

.item {
white-space: nowrap;
text-overflow: ellipsis;
overflow: hidden;
flex: 1;
text-align: left;
}

.scroll-view {
flex: 1;
}

.navi {
  color:blue;
  vertical-align: middle;
}

.bank-icon {
  width: 40rpx;
  height: 40rpx;
}